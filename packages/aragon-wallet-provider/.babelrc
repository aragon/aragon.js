{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": [
            ">2%",
            "last 1 edge versions",
            "not ie > 0",
            "not op_mini all"
          ]
        },
        "useBuiltIns": false
      }
    ],
    "stage-2"
  ],
  "plugins": [
    ["inline-json-import", { "extensions": [".json"] }],
    "transform-flow-strip-types",
    "transform-runtime"
  ],
  "env": {
    "production": {
      "presets": [
        ["minify", {
          "mangle": false
        }]
      ]
    }
  }
}
