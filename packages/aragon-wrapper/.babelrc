{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": [
            ">2%",
            "last 1 edge versions",
            "not ie > 0",
            "not op_mini all"
          ]
        },
        "useBuiltIns": false
      }
    ]
  ],
  "plugins": [
    "transform-flow-strip-types",
    "transform-function-bind",
    ["transform-object-rest-spread", { "useBuiltIns": true }],
    "transform-pipeline",
    "transform-runtime"
  ],
  "env": {
    "production": {
      "presets": ["minify"]
    }
  }
}
