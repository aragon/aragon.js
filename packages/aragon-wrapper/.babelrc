{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": [
            "> 1%",
            "last 3 versions",
            "ie >= 9",
            "ios >= 8",
            "android >= 4.2"
          ]
        },
        "useBuiltIns": false
      }
    ]
  ],
  "plugins": [
    "transform-flow-strip-types",
    "transform-function-bind",
    ["transform-object-rest-spread", { "useBuiltIns": true }],
    "transform-pipeline",
    "transform-runtime"
  ],
  "env": {
    "production": {
      "presets": ["minify"]
    }
  }
}
